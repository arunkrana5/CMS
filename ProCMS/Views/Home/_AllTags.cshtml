@model IList<BlogRelatedInformation>

@if (Model.Count > 0)
{
<h2 class="heading-20">Tags</h2>

<ul class="list-unstyled tags">
    @foreach (var item in Model)
    {
        <li>
            <a href="/@(item.TypeURL+"-Tags/"+item.URL)" class="tag-list">
                @item.Name <span class="line-space">|</span>
                             @*<span class="color-prmy">@item.BlogCount</span>*@
            </a>
        </li>
    }
</ul>
}